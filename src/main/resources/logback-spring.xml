<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%-20thread] %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

<!--  <root level="DEBUG">-->
      <root level="INFO">
    <appender-ref ref="STDOUT" />
  </root>

</configuration>





